{
  "name": "express-rest-starter",
  "version": "1.0.0",
  "description": "Basic expressjs rest api starter with mongodb",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "start:prod": "pm2 start ./src/server.js",
    "watch-server": "nodemon -r dotenv/config src/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-server\"",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mustafabalila/expressjs-restapi-starter.git"
  },
  "author": "mustafabalila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mustafabalila/expressjs-restapi-starter.git/issues"
  },
  "homepage": "https://github.com/mustafabalila/expressjs-restapi-starter.git#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.6.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "uuidv4": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^5.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "mocha": "^10.2.0",
    "nodemon": "3.0.2"
  }
}
